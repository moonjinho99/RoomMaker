<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper

  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"

  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.rm.mapper.RoomMapper">


<insert id="makeRoom">
	insert into room(type,id) values(#{type},#{id})
</insert>

<insert id="makeRoomDetail">
	insert into roomdetail(title,roompw,member_cnt,img,explanation) values(#{title}, #{roompw}, #{member_cnt}, #{main_img}, #{explanation}) 
</insert>

</mapper>